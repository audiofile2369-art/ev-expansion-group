{
  "rewrites": [
    {
      "source": "/api/:path*",
      "has": [{ "type": "host", "value": "portal.evexpansiongroup.com" }],
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "portal.evexpansiongroup.com" }],
      "destination": "/portal/$1"
    }
  ]
}
